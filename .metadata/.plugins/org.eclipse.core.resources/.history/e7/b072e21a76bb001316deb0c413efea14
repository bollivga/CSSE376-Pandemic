import static org.junit.Assert.*;
import org.junit.Before;
import org.junit.Test;


public class CityGraphTest {

	

	@Test
	public void test() {
		CityGraph tempGraph = new CityGraph();
		assertTrue(tempGraph.getCity("New York").isConnectedTo(tempGraph.getCity("Washington")));
		assertTrue(tempGraph.getCity("Chicago").isConnectedTo(tempGraph.getCity("Atlanta")));
		tempGraph.getCity("Kinshasa").infectThrice();
		assertTrue(tempGraph.getCity("Kinshasa").infectOnce());
	}

}
